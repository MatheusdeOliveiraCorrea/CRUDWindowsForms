<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
   <Dialog id="DialogoDeletar" title="Você tem certeza que deseja deletar permanentemente este usuário?">
      
      <endButton>
         <Button text="{i18n>BotaoDialogoCancelar}" type="Ghost" press="onCancelarDialogo"/>
      </endButton>   
   
      <beginButton>
         <Button text="{i18n>BotaoDialogoDeletar}" type="Emphasized" press="onDeletarUsuarioDialogo"/>
      </beginButton>

   </Dialog>
</core:FragmentDefinition>